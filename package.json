{
  "name": "muitils",
  "version": "1.0.0",
  "description": "A library of useful MUI component wrappers and utilities.",
  "main": "dist/index.js",
  "type": "module",
  "module": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./Button": "./dist/Button/index.js",
    "./Container": "./dist/Container/index.js",
    "./Field": "./dist/Field/index.js",
    "./Flex": "./dist/Flex/index.js",
    "./Form": "./dist/Form.js"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "create-docs": "typedoc --out ./docs ./src/index.ts",
    "deploy": "npm run build && npm run create-docs && npm publish",
    "start-test-app": "cd ./tests/test-app && npm start && cd ../../",
    "integration": "jest integration --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelmunson/muitils"
  },
  "keywords": [
    "mui",
    "utilities",
    "react"
  ],
  "author": "Michael Munson",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.4",
    "@mui/material": "^6.1.4",
    "@mui/x-date-pickers": "^7.21.0",
    "dayjs": "^1.11.13"
  },
  "devDependencies": {
    "typescript": "^5.6.3"
  }
}
