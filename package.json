{
  "name": "muitils",
  "version": "1.0.3",
  "description": "A library of useful MUI component wrappers and utilities.",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
